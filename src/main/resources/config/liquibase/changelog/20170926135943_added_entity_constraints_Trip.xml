<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Trip.
    -->
    <changeSet id="20170926135943-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="routes_id"
                                 baseTableName="trip"
                                 constraintName="fk_trip_routes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="routes"/>

        <addForeignKeyConstraint baseColumnNames="trip_master_id"
                                 baseTableName="trip"
                                 constraintName="fk_trip_trip_master_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
